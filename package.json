  "scripts": {
    "clean": "rimraf ./build",
    "start": "webpack serve --env=development --progress --profile --content-base src/",
    "build": "npm run clean && npm run build:bundle && npm run build:gz",
    "build:bundle": "cross-env NODE_ENV=production webpack --env=production",
    "build:gz": "zip -r ./build.zip ./build/*",
    "test": "npm run lint && npm run test:coverage",
    "test:ci": "npm run lint && npm run test:coverage:ci",
    "test:coverage": "jest --coverage --coverageReporters=lcov --runInBand --detectOpenHandles",
    "test:coverage:ci": "jest --coverage --coverageReporters=lcov --maxWorkers=50%",
    "test:tests": "jest --config --maxWorkers=50%",
    "test:postbuild": "jest --testMatch test/build.test.js",
    "lint": "npm run lint:eslint && npm run lint:stylelint",
    "lint:eslint": "eslint \"./src/**/*.{js,snap}\"",
    "lint:stylelint": "stylelint ./src/**/*.css",
    "semantic-release": "semantic-release"
  },
