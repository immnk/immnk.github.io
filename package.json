{
  "name": "webpack-testing",
  "version": "0.0.4",
  "description": "test webpack functionality",
  "main": "index.js",
  "scripts": {
    "build:watch": "webpack --progress --color --watch --display-error-details",
    "serve": "lite-server -c=bs-config.json",
    "build:prod": "webpack --mode=production --verbose",
    "start": "npm i && concurrently \"npm run build:watch\" \"npm run serve\"",
    "test": "nyc --reporter=html --reporter=text mocha",
    "coverage": "NODE_ENV=test nyc report --reporter=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/immnk/webpack-testing.git"
  },
  "keywords": [
    "webpack",
    "module"
  ],
  "author": "Mani",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/immnk/webpack-testing/issues"
  },
  "homepage": "https://github.com/immnk/webpack-testing#readme",
  "dependencies": {},
  "devDependencies": {
    "chai": "^4.2.0",
    "concurrently": "^4.1.0",
    "copy-webpack-plugin": "^5.1.1",
    "coveralls": "^3.0.2",
    "css-loader": "^2.1.0",
    "eslint": "^5.12.0",
    "file-loader": "^3.0.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^4.3.0",
    "jsdom": "^13.1.0",
    "jsdom-global": "^3.0.2",
    "lite-server": "^2.5.4",
    "mini-css-extract-plugin": "^0.5.0",
    "mocha": "^7.1.1",
    "node-sass": "^4.13.1",
    "nyc": "^15.0.1",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "require-hooks": "0.0.6",
    "sass-loader": "^7.1.0",
    "terser-webpack-plugin": "^4.2.2",
    "url-loader": "^4.1.0",
    "webpack": "^4.28.4",
    "webpack-cli": "^3.2.1"
  }
}
